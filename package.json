  "scripts": {
    "run:server:local": "npx nodemon src/server.ts --watch 'src/' -e ts",
    "copy:client": "cp -r src/client/my-app/build dist/client/my-app",
    "build:client": "cd src/client/my-app && npm install && npm run build",
    "build:server": "npx tsc && npm run copy:client",
    "compile": "npm run build:client && npm run build:server",
    "start": "npm run compile && node ./dist/server.js",
