{
  "name": "calculation",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run compile && node ./dist/server.js",
    "test": "npx jest --coverage",
    "lint": "npm run lint:client && npm run lint:server",
    "compile": "npm run build:client && npm run build:server",
    "build:client": "cd src/client/my-app && npm install && npm run build",
    "build:server": "npx tsc && npm run copy:client",
    "lint:client": "cd src/client/my-app && npm run lint",
    "lint:server": "eslint src/**/*.ts",
    "run:server:local": "npx nodemon src/server.ts --watch 'src/' -e ts",
    "copy:client": "cp -r src/client/my-app/build dist/client/my-app"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.8",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.12",
    "@types/morgan": "^1.9.9",
    "@types/nock": "^11.1.0",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "eslint": "^8.57.0",
    "globals": "^15.0.0",
    "jest": "^29.7.0",
    "nock": "^13.5.4",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4",
    "typescript-eslint": "^7.5.0"
  }
}
